<script type='text/javascript'>
    var chart_{{id}} = echarts.init(
        document.getElementById('{{id}}'),
        {{{themeOrNull}}},
    {{#initOpts}}
        {
            {{#renderer}}renderer: '{{.}}'{{/renderer}}
        }
    {{/initOpts}}   );
    var option_{{id}} = {
{{#option}}
    {{#title}}
        title: {
            text: '{{.}}'
        },
    {{/title}}
    {{#tooltip}}
        tooltip: {
        {{#trigger}}
            trigger: '{{.}}',
        {{/trigger}}
        {{#axisPointer}}
            axisPointer: {
            {{#type}}
                type: '{{.}}',
            {{/type}}
            }
        {{/axisPointer}}
        },
    {{/tooltip}}
    {{#toolbox}}
        toolbox: {
            feature: {
            {{#dataZoom}}
                dataZoom: {},
            {{/dataZoom}}
            {{#saveAsImage}}
                saveAsImage: {
                {{#pixelRatio}}
                    pixelRatio: {{.}}
                {{/pixelRatio}}
                },
            {{/saveAsImage}}
            {{#restore}}
                restore: {}
            {{/restore}}
            }
        },
    {{/toolbox}}
    {{#legend}}
        legend: {},
    {{/legend}}
    {{#dataset}}
        dataset: {
            source: [
                {{#rows}}
                [{{#columns}}{{{quotedValue}}}{{^last}},{{/last}}{{/columns}}]{{^last}},{{/last}}
                {{/rows}}
            ]
        },
    {{/dataset}}
    {{#xAxis}}
        xAxis: {
        {{#alignTicksPresent}}
            alignTicks: {{alignTicks}},
        {{/alignTicksPresent}}
        {{#axisLabel}}
            axisLabel: {
            {{#formatter}}
                formatter: '{{.}}'
            {{/formatter}}
            },
        {{/axisLabel}}
        {{#axisLine}}
            axisLine: {
                show: {{show}}
            },
        {{/axisLine}}
        {{#noBoundaryGap}}
            boundaryGap: false,
        {{/noBoundaryGap}}
        {{#name}}
            name: {{{quotedName}}},
        {{/name}}
        {{#offset}}
            offset: {{.}},
        {{/offset}}
        {{#position}}
            position: '{{.}}',
        {{/position}}
        {{#type}}
            type: '{{.}}'
        {{/type}}
        },
    {{/xAxis}}
    {{#yAxis}}
        yAxis: {
        {{#alignTicksPresent}}
            alignTicks: {{alignTicks}},
        {{/alignTicksPresent}}
        {{#axisLabel}}
            axisLabel: {
            {{#formatter}}
                formatter: '{{.}}'
            {{/formatter}}
            },
        {{/axisLabel}}
        {{#axisLine}}
            axisLine: {
                show: {{show}}
            },
        {{/axisLine}}
        {{#name}}
            name: {{{quotedName}}},
        {{/name}}
        {{#offset}}
            offset: {{.}},
        {{/offset}}
        {{#position}}
            position: '{{.}}',
        {{/position}}
        {{#type}}
            type: '{{.}}'
        {{/type}}
        },
    {{/yAxis}}
        series: [
            {{#series}}
            {
                name: '{{name}}',
            {{#areaStyle}}
                areaStyle: {},
            {{/areaStyle}}
            {{#encode}}
                encode: { x: {{x}}, y: {{y}} },
            {{/encode}}
            {{#label}}
                label: {
                    show: true,
                {{#formatter}}
                    formatter: '{{.}}',
                {{/formatter}}
                {{#position}}
                    position: '{{.}}'
                {{/position}}
                },
            {{/label}}
            {{#seriesLayoutBy}}
                seriesLayoutBy: '{{.}}',
            {{/seriesLayoutBy}}
            {{#dontShowSymbol}}
                showSymbol: false,
            {{/dontShowSymbol}}
            {{#smooth}}
                smooth: true,
            {{/smooth}}
            {{#stack}}
                stack: 'total',
            {{/stack}}
                type: '{{type}}'
            }{{^last}},{{/last}}
            {{/series}}
        ]
{{/option}}
    };
    option_{{id}} && chart_{{id}}.setOption(option_{{id}});</script>